{
  "expo": {
    "name": "weather",
    "slug": "weather-saloum",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/iconMeteo.png",
    "splash": {
      "image": "./assets/meteo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier":"com.sylla.weather",
      "supportsTablet": true
    },
    "android": {
      "package":"com.sylla.weather",
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconMeteo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/iconMeteo.png"
    }
  }
}